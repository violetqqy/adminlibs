{
  "compilerOptions": {
    "declaration": true,
    "stripInternal": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "module": "es2015",
    "moduleResolution": "node",
    "outDir": "../../core",
    "rootDir": ".",
    "target": "es2015",
    "lib": ["es2015", "dom"],
    "sourceMap": true,
    "inlineSources": true,
    "lib": ["es2015", "dom"],
    "skipLibCheck": true,
    "types": [],
    "baseUrl": ".",
    "paths": {
      "adminlibs/core": ["../../core/index"]
    }
  },
  "files": [
    "index.ts",
    "typings.d.ts"
  ],
  "angularCompilerOptions": {
    "genDir": "../../core",
    "skipMetadataEmit": true,
    "annotateForClosureCompiler": true,
    "flatModuleOutFile": "index.js",
    "flatModuleId": "adminlibs/core",
    "skipTemplateCodegen": true
  }
}
